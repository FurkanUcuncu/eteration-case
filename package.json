{
  "name": "eteration-case",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "test": "jest --verbose --coverage"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.2",
    "babel-jest": "^29.7.0",
    "expo": "~49.0.15",
    "expo-app-loading": "^2.1.1",
    "expo-font": "~11.4.0",
    "expo-splash-screen": "^0.20.5",
    "expo-status-bar": "~1.6.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^4.12.3",
    "react-native-reanimated": "~3.3.0",
    "react-native-redash": "^18.1.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.10",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.7",
    "enzyme": "^3.11.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "^49.0.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": ["\\.snap$", "<rootDir>/node_modules/", "<rootDir>/assets/"],
    "transformIgnorePatterns": ["node_modules/?!(static-container)", "jest-runner"],
    "collectCoverageFrom": ["src/**/*.{ts,tsx}", "!**/node_modules/**","!**/src/models/**"]
  },
  "private": true
}
